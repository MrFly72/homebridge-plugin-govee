{
  "pluginAlias": "GoveeHomebridgePlugin",
  "pluginType": "platform",
  "singular": true,
  "schema": {
    "type": "object",
    "properties": {
      "name": {
        "title": "Name",
        "type": "string",
        "required": true,
        "default": "Govee"
      }, 
      "batteryThreshold": {
        "title": "Battery Threshold",
        "type": "number",
        "default": "25"
      },
      "debug": {
        "title": "Debug",
        "type": "boolean",
        "default": "false"
      },
      "humidityOffset": {
        "title": "Humidity Offset",
        "type": "number",
        "default": "0"
      },
      "IgnoreDeviceNames": {
        "title": "Ignored DeviceNames",
        "type": "array",
          "items": {
         "title": "DeviceName",
         "type": "string"
       }
     }
    }
  }
}